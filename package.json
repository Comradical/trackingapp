{
  "name": "Tracking-Hawk-api",
  "version": "1.0.0",
  "private": "true",
  "main": "app/index.js",
  "scripts": {
    "start": "node bin/prod",
    "dev": "nodemon --exec babel-node bin/dev",
    "prestart": "babel app --out-dir dist --copy-files",
    "build": "npm run clean && mkdir dist && babel app -s -d dist",
    "clean": "rm -rf dist",
    "production": "npm run build && NODE_ENV=Production node bin/prod",
    "test": "cross-env ENVIRONMENT=TESTING NODE_ENV=Test nyc mocha --recursive --timeout 15000 --require babel-core/register"
  },
  "nyc": {
    "lines": 75,
    "statements": 75,
    "functions": 70,
    "require": [
      "babel-register"
    ],
    "exclude": [
      "**/examplecode.js",
      "**/*.test.js",
      ".nyc_output",
      "coverage",
      "node_modules"
    ],
    "reporter": [
      "json",
      "lcov",
      "text-summary"
    ],
    "sourceMap": true,
    "cache": true,
    "instrument": true,
    "check-coverage": false
  },
  "author": "brianaustgen@gmail.com",
  "license": "UNLICENSED",
  "dependencies": {
    "ajv": "latest",
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "ejs": "^2.6.1",
    "expect": "^22.4.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "http-status-codes": "^1.3.0",
    "log4js": "^2.5.3",
    "method-override": "^2.3.10",
    "mongo-join-query": "^1.0.2",
    "mongoose": "^5.3.4",
    "mongoose-type-url": "^1.0.2",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "query-string": "^5.0.1",
    "referee": "^1.2.0",
    "request": "^2.87.0",
    "requestretry": "latest",
    "serve-favicon": "^2.5.0",
    "sinon-chai": "^2.14.0",
    "underscore": "latest",
    "xml2json": "^0.11.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cross-env": "^5.1.4",
    "eslint": "^4.16.0",
    "istanbul": "^0.4.5",
    "log4js-test-appender": "^0.1.1",
    "logger": "0.0.1",
    "mocha": "^5.0.4",
    "mocha-standard": "^1.0.0",
    "nock": "^9.2.3",
    "nodemon": "^1.14.11",
    "nyc": "^10",
    "proxyquire": "^1.8.0",
    "sinon": "^4.1.6",
    "sinon-stub-promise": "^4.0.0",
    "standard": "^10.0.3",
    "supertest": "^3.0.0"
  },
  "files": [
    "app/index.js"
  ]
}
